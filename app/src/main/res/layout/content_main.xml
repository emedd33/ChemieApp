<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_main"
    android:backgroundTint="@android:color/background_dark"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.eskild.hc.HovedActivity"
    tools:showIn="@layout/app_bar_main">


    <GridLayout
        android:id="@+id/hoved_gridlayout"
        android:layout_width="368dp"
        android:layout_height="495dp"
        android:paddingLeft="50dp"
        android:paddingRight="50dp"
        tools:layout_editor_absoluteX="11dp"
        tools:layout_editor_absoluteY="11dp">

        <ImageButton
            android:id="@+id/sladreboksen_button"
            android:layout_width="125dp"
            android:layout_height="125dp"
            android:onClick="onClick"
            app:srcCompat="@mipmap/sladreboksen_icon" />

        <ImageButton
            android:id="@+id/cake_button"
            android:layout_width="125dp"
            android:layout_height="125dp"
            android:layout_column="4"
            android:onClick="onClick"
            android:layout_row="0"
            app:srcCompat="@mipmap/cake_icon" />

        <ImageButton
            android:id="@+id/event_button"
            android:layout_width="125dp"
            android:layout_height="125dp"
            android:layout_column="0"
            android:layout_row="1"
            android:onClick="onClick"
            app:srcCompat="@mipmap/event_icon_white" />
    </GridLayout>

</android.support.constraint.ConstraintLayout>
